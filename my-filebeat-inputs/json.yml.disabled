- type: log
  paths:
    - /opt/testlog/status_*.json
  fields:
    env: dev
    type: player
    location: chancery
    log: nuberlog
  backoff: 0.05s
  max_backoff: 1s
  ignore_older: 30m
  tail_files: true
  multiline.pattern: "^([12]\\d{3}[-_ ](0[1-9]|1[0-2])[-_ ](0[1-9]|[12]\\d|3[01]))"
  multiline.negate: true
  multiline.match: after
